<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.group">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login" />
        <activity android:name=".private_activity" />
        <activity
            android:name=".publicVehicles"
            android:label="@string/title_activity_public_vehicles"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".privateVehicles"
            android:label="@string/title_activity_private_vehicles"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".generalVehicles"
            android:label="@string/title_activity_general_vehicles"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".motorists"
            android:label="@string/title_activity_motorists"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".VehiclesActivity"></activity>
    </application>

</manifest>